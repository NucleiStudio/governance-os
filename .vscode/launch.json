    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'governance-os-pallet-bylaws'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=governance-os-pallet-bylaws"
                ],
                "filter": {
                    "name": "governance-os-pallet-bylaws",
                    "kind": "lib"
                }
            },
            "args": [
                "default_bylaw_if_no_bylaw_registered"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}